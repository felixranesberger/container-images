{
  "name": "container-images",
  "type": "module",
  "version": "0.1.8",
  "description": "Container queries for responsive image loading",
  "author": "Felix Ranesberger <felix@ranesberger.de>",
  "license": "MIT",
  "homepage": "https://github.com/felixranesberger/container-images#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixranesberger/container-images.git"
  },
  "bugs": "https://github.com/felixranesberger/container-images/issues",
  "keywords": [
    "container",
    "image",
    "responsive",
    "lazy",
    "lazyloading",
    "responsive image loading",
    "query"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/node": "^18.14.1",
    "bumpp": "^9.0.0",
    "eslint": "^8.34.0",
    "esno": "^0.16.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.2",
    "rimraf": "^5.0.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vite": "^4.1.4",
    "vitest": "^0.28.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
