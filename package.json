{
  "name": "container-images",
  "type": "module",
  "version": "1.0.0",
  "description": "Container queries for responsive image loading",
  "author": "Felix Ranesberger <felix@ranesberger.de>",
  "license": "MIT",
  "homepage": "https://github.com/felixranesberger/container-images#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixranesberger/container-images.git"
  },
  "bugs": "https://github.com/felixranesberger/container-images/issues",
  "keywords": [
    "container",
    "image",
    "responsive",
    "lazy",
    "lazyloading",
    "responsive image loading",
    "query"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@antfu/utils": "0.7.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.40.2",
    "@antfu/ni": "0.21.5",
    "@types/node": "18.17.3",
    "bumpp": "9.1.1",
    "eslint": "8.46.0",
    "esno": "0.17.0",
    "jsdom": "22.1.0",
    "lint-staged": "13.2.3",
    "rimraf": "5.0.1",
    "simple-git-hooks": "2.9.0",
    "typescript": "5.1.6",
    "unbuild": "1.2.1",
    "vite": "4.4.9",
    "vitest": "0.34.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
