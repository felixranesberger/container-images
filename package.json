{
  "name": "container-images",
  "type": "module",
  "version": "0.1.9",
  "description": "Container queries for responsive image loading",
  "author": "Felix Ranesberger <felix@ranesberger.de>",
  "license": "MIT",
  "homepage": "https://github.com/felixranesberger/container-images#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felixranesberger/container-images.git"
  },
  "bugs": "https://github.com/felixranesberger/container-images/issues",
  "keywords": [
    "container",
    "image",
    "responsive",
    "lazy",
    "lazyloading",
    "responsive image loading",
    "query"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "require": "./dist/index.umd.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/lib/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.37.0",
    "@types/node": "^18.11.9",
    "eslint": "^8.31.0",
    "sass": "^1.56.1",
    "stylelint": "^15.3.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-scss": "^4.5.0",
    "stylelint-use-logical": "^2.1.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^2.0.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^4.1.6"
  }
}
